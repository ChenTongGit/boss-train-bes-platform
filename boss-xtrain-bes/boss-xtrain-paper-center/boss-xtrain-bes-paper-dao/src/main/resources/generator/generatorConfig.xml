<!DOCTYPE generatorConfiguration
        PUBLIC "-//mybatis.org//DTD MyBatis Generator Configuration 1.0//EN"
        "http://mybatis.org/dtd/mybatis-generator-config_1_0.dtd">

<!--suppress MybatisGenerateCustomPluginInspection -->
<generatorConfiguration>

<!--    <properties resource="db.properties"/>-->

    <context subjectId="Mysql" targetRuntime="MyBatis3Simple" defaultModelType="flat">

        <property name="javaFileEncoding" value="UTF-8"/>
        <property name="useMapperCommentGenerator" value="false"/>

        <plugin type="tk.mybatis.mapper.generator.MapperPlugin">
            <property name="mappers" value="com.boss.xtrain.paper.CommonMapper"/>
            <property name="caseSensitive" value="true"/>
            <property name="forceAnnotation" value="true"/>
            <property name="beginningDelimiter" value="`"/>
            <property name="endingDelimiter" value="`"/>
        </plugin>
        <commentGenerator>
            <property name="suppressDate" value="true"/>
            <!-- 是否去除自动生成的注释 true：是 ： false:否 -->
            <property name="suppressAllComments" value="true"/>
        </commentGenerator>
        <jdbcConnection driverClass="com.mysql.cj.jdbc.Driver"
                        connectionURL="jdbc:mysql://39.101.178.139:3306/db_exam_center?serverTimezone=Asia/Shanghai&amp;useSSL=true&amp;useUnicode=true&amp;characterEncoding=utf-8"
                        userId="root"
                        password="zzy873190934">
        </jdbcConnection>
        <javaTypeResolver>
            <property name="forceBigDecimals" value="false" />
        </javaTypeResolver>
        <!--MyBatis 生成器只需要生成 Model-->

        <javaModelGenerator targetPackage="com.boss.bes.paper.entity"
                            targetProject="src/main/java"/>

        <sqlMapGenerator targetPackage="mapper"
                         targetProject="src/main/resources"/>

        <javaClientGenerator targetPackage="com.boss.bes.paper.mapper"
                             targetProject="src/main/java"
                             type="XMLMAPPER"/>

        <table tableName="t_paper">
            <generatedKey column="paper_id" sqlStatement="JDBC"/>
        </table>
        <table tableName="t_paper_subject">
            <generatedKey column="paper_subject_id" sqlStatement="JDBC"/>
        </table>
        <table tableName="t_paper_subject_answer">
            <generatedKey column="paper_subject_answer_id" sqlStatement="JDBC"/>
        </table>
    </context>
</generatorConfiguration>
